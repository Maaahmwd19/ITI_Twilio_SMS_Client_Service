<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>AdminServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.AdminServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.LoginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.LogoutServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ProfileServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.ProfileServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegistrationServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.RegistrationServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UserSessionServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.UserSessionServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SMSHistoryServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.SMSHistoryServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteSMSServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.DeleteSMSServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateProfileServlet</servlet-name>
        <servlet-class>com.iti.project.TwilioSMSClient.servlets.UpdateProfileServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>AdminServlet</servlet-name>
        <url-pattern>/AdminServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginServlet</servlet-name>
        <url-pattern>/LoginServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutServlet</servlet-name>
        <url-pattern>/LogoutServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ProfileServlet</servlet-name>
        <url-pattern>/ProfileServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegistrationServlet</servlet-name>
        <url-pattern>/RegistrationServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UserSessionServlet</servlet-name>
        <url-pattern>/UserSessionServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SMSHistoryServlet</servlet-name>
        <url-pattern>/SMSHistoryServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteSMSServlet</servlet-name>
        <url-pattern>/DeleteSMSServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateProfileServlet</servlet-name>
        <url-pattern>/UpdateProfileServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
